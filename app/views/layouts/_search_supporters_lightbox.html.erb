        <li><%= link_to '41〜45歳', search_supporters_path(min_age: 41, max_age: 45), method: :post %></li>
        <li><%= link_to '46〜50歳', search_supporters_path(min_age: 46, max_age: 50), method: :post %></li>
        <li><%= link_to '51〜55歳', search_supporters_path(min_age: 51, max_age: 55), method: :post %></li>
        <li><%= link_to '55〜60歳', search_supporters_path(min_age: 56, max_age: 60), method: :post %></li>
        <li><%= link_to '61歳以上', search_supporters_path(min_age: 61), method: :post %></li>
      </ul>
    </div>
    <div class="col-xs-5">
      <label class="ul">時期から検索</label>
