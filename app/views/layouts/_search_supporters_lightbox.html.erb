        <li><%= link_to '小学校低学年（小１～小３）', '#', method: :post %></li>
        <li><%= link_to '小学校高学年（小４～小６）', '#', method: :post %></li>
        <li><%= link_to '中学１年生', '#', method: :post %></li>
        <li><%= link_to '中学２年生', '#', method: :post %></li>
        <li><%= link_to '中学３年生', '#', method: :post %></li>
        <li><%= link_to '高校生', '#', method: :post %></li>
      </ul>
    </div>
</div>
